          "_customTemplate": true,
          "_afterNavbarItem": false,
          "_customHTML": "<!-- Begin MailChimp Signup Form -->\n<div id=\"mailchimp\">\n<form action=\"https://livininspired.us13.list-manage.com/subscribe/post?u=223599d32c00e5dc8988a5e4e&amp;id=5766d71a11\" method=\"post\" id=\"mc-embedded-subscribe-form\" name=\"mc-embedded-subscribe-form\" class=\"validate\" target=\"_blank\" novalidate=\"\">\n    <div id=\"mc_embed_signup_scroll\">\n\t<center><h2 class=\"mbr-section-title mbr-fonts-style align-center\">Stay in touch! Interested in our latest retreats and workshops?\n        </h2><h3 class=\"mbr-section-title mbr-fonts-style align-center\">Leave us your email address and we will let you know</h3>\n\t\t\t<input type=\"text\" size=\"”30″\" value=\"Your Name\" name=\"FNAME\" class=\"\" id=\"mce-FNAME\" onfocus=\"if(this.value==this.defaultValue)this.value='';\" onblur=\"if(this.value=='')this.value=this.defaultValue;\">\n\n<input type=\"email\" size=\"”30″\" value=\"Your Email\" name=\"EMAIL\" class=\"required email\" id=\"mce-EMAIL\" onfocus=\"if(this.value==this.defaultValue)this.value='';\" onblur=\"if(this.value=='')this.value=this.defaultValue;\">  \n        <input type=\"submit\" value=\"YES! I AM IN!\" name=\"subscribe\" id=\"mc-embedded-subscribe\" class=\"button\"></center>\n    </div>\n</form>\n</div>\n\n<!--End mc_embed_signup-->",
          "_customCSS": "#mailchimp {\n\tbackground: #85bfd0;\n\tcolor: #3D6392;\n\tpadding: 20px 15px;\n}\n\t#mailchimp input {\n\t\t border: medium none;\n    color: gray;\n    font-family: ;\n    font-size: 16px;\n    font-style: ;\n    margin-bottom: 10px;\n    padding: 8px 10px;\n    width: 600px;\n\t\tborder-radius: 20px;\n\t\t-moz-border-radius: 20px;\n\t\t-webkit-border-radius: 20px;\n\t}\n\t\t#mailchimp input.email { background: #fff }\n\t\t#mailchimp input.name { background: #fff}\n\t\t#mailchimp input[type=\"submit\"] {\n\t\t\tbackground: #115B87;\n\t\t\tcolor: #fff;\n\t\t\tcursor: pointer;\n\t\t\tfont-size: 15px;\n\t\t\twidth: 35%;\n\t\t\tpadding: 8px 0;\n\n\t\t}\nbackground: #85bfd0;\n\tcolor: #3D6392;\n\tpadding: 20px 15px;\n\t\t\t#mailchimp input[type=\"submit\"]:hover { color: #F2C94C }",
